# Environment configuration for Abdullah Junior Mobile App

# Backend API Configuration
API_BASE_URL=https://abdullah-junior-api.fly.dev

# Gemini API Configuration (if direct access needed)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash

# Notification Settings
FCM_ENABLED=true
NOTIFICATION_CHANNELS=approvals,suggestions,digest

# Feature Flags
ENABLE_OFFLINE_MODE=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_CHAT_HISTORY=true

# Theme Settings
DEFAULT_THEME=dark